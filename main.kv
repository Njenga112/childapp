<Screen1>:
    name: "one"
    nam:nam
    age:age
    grade:grade
    hobby:hobby
    Image:
        source: 'images/image2.jpg'
        allow_stretch: True
        keep_ratio: False

    FloatLayout:
        Label:
            text: "CHILD LEARNING APP"
            font_size: 32
            pos_hint: {"center_x":0.5,"center_y":0.95}
            bold:True
            color: 0.5,0.5,1,0.9
    GridLayout:
        cols: 2
        row_force_default:True
        row_default_height: 100
        minimum_height: 0.5



        Label:
        Label:

        Label:
            text: "NAME"
            font_size: 32
        TextInput:
            id: nam
            hint_text: "ENTER YOUR  NAME"
            font_size: 32
            multiline: False
            background_color:0.5,0.5,1,0.2

        Label:
            text: "AGE"
            font_size: 32
        TextInput:
            id: age
            hint_text: "Enter Your AGE"
            font_size: 32
            multiline: False
            background_color: 0.5,0.5,1,0.2
        Label:
            text: "GRADE"
            font_size: 32
        TextInput:
            id: grade
            hint_text: "Enter Your GRADE"
            font_size: 32
            multiline: False
            background_color: 0.5,0.5,1,0.2
        Label:
            text: "HOBBY"
            font_size: 32
        TextInput:
            id: hobby
            hint_text: "Enter Your HOBBY"
            font_size: 32
            multiline: False
            background_color: 0.5,0.5,1,0.2
        Button:
            text: "START LEARNING>>"
            font_size: 32
            background_color: 0,0,0,0
            on_release:
                root.on_validate()
                root.manager.transition.direction = "left"

<Screen2>:
    name: "two"
    wel:wel
    Image:
        source: 'images/image3.jpg'
        allow_stretch: True
        keep_ratio: False
    BoxLayout:
        orientation: "vertical"
        size: root.width, root.height
        Label:
            id: wel
            text: "WELCOME"
            italic: True
            bold: True
            color: 1,0,0,0.8
            font_size: 34
        Label:
            text: "IT'S LEARNING TIME!!"
            font_size: 34
            italic: True
            color: 1,0,0,0.8


        Button:
            text: "NUMBERS"
            font_size: 32
            color: 1,0,0,0.5
            background_color: 0,0,0,0
            on_release:
                app.root.current = "three"
                root.manager.transition.direction = "left"
        Button:
            text: "PRONUNCIATION"
            font_size: 32
            background_color: 0,0,0,0
            color: 1,0,0,0.5
            on_release:
                app.root.current = "four"
                root.manager.transition.direction = "left"
        Button:
            text: "WRITING"
            font_size: 32
            color: 1,0,0,0.5
            background_color: 0,0,0,0
        Button:
            text: "COLOURS"
            color: 1,0,0,0.5
            font_size: 32
            background_color: 0,0,0,0
        Button:
            text: "SHAPES"
            font_size: 32
            color: 1,0,0,0.5
            background_color: 0,0,0,0
        Button:
            text: "SONGS"
            font_size: 32
            color: 1,0,0,0.5
            background_color: 0,0,0,0
            on_release:
                app.root.current = "eight"
                root.manager.transition.direction = "left"
        Button:
            text: "<<BACK"
            font_size: 28
            background_color: 0,0,0,0
            pos_hint: {"center_x": 0.2, "center_y": 0.1}
            on_release:
                app.root.current = "one"
                root.manager.transition.direction = "right"

