<InvalidInfo>:
    canvas.before:
        Color:
            rgba: (0,0,1,0.2)
        Rectangle:
            size: self.size
            pos: self.pos
    Label:
        text : "Please enter valid information"
        font_size: 14
        size_hint : 0.1, 0.1
        pos_hint : {"x" : 0.9, "top" : 0.9}
